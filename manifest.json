{
    "name": "Secure Password Generator",
    "version": "1.0.0",
    "permissions": [
        "tabs",
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "generator.html"
    },
    "manifest_version": 3
}