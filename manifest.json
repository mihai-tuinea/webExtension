{
    "name": "Secure Password Generator",
    "version": "1.0.0",
    "permissions": [
        "storage",
        "tabs"
    ],
    "background": {
        "service-worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ],
    "action": {
        "default_popup": "generator.html"
    },
    "manifest_version": 3
}